<annotation>
	<folder>${data.folder}</folder>
	<filename>${data.filename}</filename>
	<path>${data.path}</path>
	<source>
		<database>Unknown</database>
	</source>
	<size>
		<width>${data.width}</width>
		<height>${data.height}</height>
		<depth>3</depth>
	</size>
	<segmented>0</segmented>
	%for o in data.objects:
	<object>
		<name>${o.name}</name>
		<pose>Frontal</pose>
		<truncated>0</truncated>
		<difficult>0</difficult>
		<occluded>0</occluded>
		<bndbox>
			<xmin>${o.xmin}</xmin>
			<xmax>${o.xmax}</xmax>
			<ymin>${o.ymin}</ymin>
			<ymax>${o.ymax}</ymax>
		</bndbox>
	</object>
	%endfor
</annotation>